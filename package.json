{
  "name": "thelemapi",
  "version": "1.0.0",
  "description": "A sample API using Node.js best practices",
  "main": "server.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon server.js",
    "start": "NODE_ENV=production node server.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest coverage",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "format": "prettier \"**/*.+(js|json|less|css|ts|tsx|md)\" --write",
    "check-updates": "ncu",
    "update": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seb7887/thelemapi.git"
  },
  "author": "Sebastian Matias Segura (@seb7887)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seb7887/thelemapi/issues"
  },
  "homepage": "https://github.com/seb7887/thelemapi#readme",
  "dependencies": {
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "notarealdb": "^0.2.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-security": "^1.4.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^24.3.1",
    "jest-watch-typeahead": "^0.2.1",
    "nodemon": "^1.18.10",
    "npm-check-updates": "^3.0.2",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2"
  }
}
